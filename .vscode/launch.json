{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Run infra-cli",
      "type": "debugpy",
      "request": "launch",
      "module": "infra_lib.cli.cli",
      "args": ["init", "--stack", "net8", "--provider", "aws"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      }
    }
  ]
}
